<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" >
        <layer-list>
            <item android:drawable="@drawable/img_uncheck"
                android:height="35dp" android:width="35dp"/>
        </layer-list>
    </item>
    <item android:state_checked="true" >
        <layer-list>
            <item android:drawable="@drawable/img_check"
                android:height="35dp" android:width="35dp"/>
        </layer-list>
    </item>
</selector>